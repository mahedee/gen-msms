@model IEnumerable<MSMS.Web.Models.FeeSettings>

@{
    ViewBag.Title = "Index";
}

<h3>Monthly Fee Settings</h3>

<p>
    @*@Html.ActionLink("Create New", "Create")*@
    <a class="button" href="~/FeeSettings/Create">Fee Settings</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClassInfo.ClassName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MonthlyFee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EffectiveDate)
        </th>
        <th>Option</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClassInfo.ClassName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MonthlyFee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EffectiveDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
